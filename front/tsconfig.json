{
  "compilerOptions": {
    "esModuleInterop": true,
    "sourceMap": true, // 에러난 위치
    "lib": ["ES2020", "DOM"],
    "jsx": "react",
    "module": "esnext",
    "moduleResolution": "Node", //최신모듈 해석
    "target": "es5",
    "strict": true,
    "resolveJsonModule": true, //모듈 가능
    "baseUrl": ".",
    "paths": {
      "@hooks/*": ["hooks/*"],
      "@components/*": ["components/*"],
      "@layouts/*": ["layouts/*"],
      "@pages/*": ["pages/*"],
      "@utils/*": ["utils/*"],
      "@typings/*": ["typings/*"]
    }
  }
}
